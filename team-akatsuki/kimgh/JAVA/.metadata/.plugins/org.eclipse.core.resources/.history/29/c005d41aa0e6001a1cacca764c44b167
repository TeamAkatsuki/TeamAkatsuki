package chap05.ex;

public class Ex02 {
	public static void main(String [] args) {
		int [][] array = {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88}
		};
		
		int sum = 0;
		double avg = 0;
		
		for (int i = 0; i < ; i++) {
			System.out.println((int)array[i]);
		}
		
		
		System.out.println("sum = " + sum);
		System.out.println("avg = " + avg);
	}

}
